{
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "trailingComma": "none",
  "printWidth": 100,
  "svelteStrictMode": false,
  "svelteBracketNewLine": false,
  "svelteAllowShorthand": true,
  "svelteIndentScriptAndStyle": false,
  "overrides": [
    {
      "files": "package/**",
      "options": {
        "semi": true,
        "printWidth": 140
      }
    },
    {
      "files": ".node_bin/**",
      "options": {
        "semi": true,
        "printWidth": 140
      }
    }
  ]
}
