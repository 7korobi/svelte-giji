{
  "name": "svelte-kit",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "test": "run-s jest",
    "jest": "jest",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.8",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "sass": "^1.32.8",
    "svelte": "^3.35.0",
    "svelte-preprocess": "^4.7.0",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3",
    "vite": "^2.1.3"
  },
  "type": "module",
  "engines": {
    "node": ">= 12.17.0"
  },
  "dependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.0",
    "fancy-date": "^0.4.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "svelte-jester": "^1.3.2",
    "svelte-petit-libs": "^0.0.4",
    "ts-jest": "^26.5.4"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
