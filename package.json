{
		"name": "svelte-kit",
		"version": "0.0.1",
    "license": "MIT",
		"scripts": {
				"test": "run-s jest",
				"jest": "jest",
				"dev": "svelte-kit dev",
				"build": "svelte-kit build",
				"start": "svelte-kit start"
		},
		"devDependencies": {
				"@sveltejs/adapter-node": "next",
				"@sveltejs/kit": "next",
				"@testing-library/jest-dom": "^5.11.9",
				"@testing-library/svelte": "^3.0.3",
				"@types/jest": "^26.0.21",
				"@types/node": "^14.14.35",
				"@types/node-fetch": "^2.5.8",
				"jest": "^26.6.3",
				"node-fetch": "^2.6.1",
				"sass": "^1.0.0",
				"svelte": "^3.29.0",
				"svelte-jester": "^1.3.1",
				"svelte-preprocess": "^4.0.0",
				"tslib": "^2.0.0",
				"typescript": "^4.0.0",
				"vite": "^2.1.0"
		},
		"type": "module",
		"engines": {
				"node": ">= 12.17.0"
		},
		"dependencies": {
				"npm-run-all": "^4.1.5",
				"ts-jest": "^26.5.4",
				"yarn": "^1.22.10"
		},
		"jest": {
				"transform": {
						"^.+\\.svelte$": [
								"svelte-jester",
								{
										"preprocess": true
								}
						],
						"^.+\\.ts$": "ts-jest"
				},
				"moduleFileExtensions": [
						"js",
						"ts",
						"svelte"
				],
				"setupFilesAfterEnv": [
						"@testing-library/jest-dom/extend-expect"
				]
		}
}
