    "fast-sort": "^3.0.3",
    "firebase": "^9.1.3",
    "js-yaml": "^4.1.0",
    "just-group-by": "^1.1.1",
    "moment": "^2.29.1",
    "mongodb": "^4.1.3",
    "pointer-tracker": "^2.5.3",
    "socket.io": "^4.3.1",
    "socket.io-client": "^4.3.2",
    "socket.io-msgpack-parser": "^3.0.1",
    "svelte2tsx": "^0.4.7",
    "tslib": "^2.3.1",
    "ua-parser-js": "^0.7.30",
    "ws": "^8.2.3"
  }
