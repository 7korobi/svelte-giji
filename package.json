{
  "name": "~TODO~",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "deploy": "run-s format build",
    "dev": "svelte-kit dev -H",
    "build": "svelte-kit build",
    "preview": "http-server build -p 3000 -b --cors -S -C ./src/lib/debug/localhost.crt -K ./src/lib/debug/localhost.key",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "validate": "run-p format lint",
    "css": "run-s make:sass",
    "make:tsc": "tsc --target es6 src/lib/scss/bin/*.ts",
    "make:sass": "node src/lib/scss/bin/index.js ./src/lib/scss/*.sass | sort | uniq > src/__spec__/__snapshots__/scss-contrast.txt",
    "make:brotli": "find public -type f -not -name \"*.br\" | grep -v \"dist/font\" | xargs -L1  brotli -Z -f -k"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "next",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.3.1",
    "prisma": "^2.26.0",
    "sass": "^1.35.1",
    "svelte": "^3.38.3",
    "svelte-preprocess": "^4.7.3",
    "typescript": "^4.3.5"
  },
  "type": "module",
  "dependencies": {
    "date-fns": "^2.22.1",
    "dexie": "^3.0.3",
    "js-yaml": "^4.1.0",
    "pointer-tracker": "^2.4.0",
    "tslib": "^2.3.0",
    "ua-parser-js": "^0.7.28"
  }
}
