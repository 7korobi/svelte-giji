    "@editorjs/editorjs": "^2.22.2",
    "@editorjs/header": "^2.6.1",
    "@editorjs/link": "^2.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "@socket.io/admin-ui": "^0.2.0",
    "date-fns": "^2.25.0",
    "debug": "^4.3.2",
    "dexie": "^3.0.3",
    "editorjs-undo": "^0.1.7",
