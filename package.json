{
  "name": "~TODO~",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "deploy": "run-s format build",
    "dev": "svelte-kit dev -H",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview -H",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "validate": "run-p format lint",
    "css": "run-s make:sass",
    "font": "run-p font:*",
    "font:logo": "svgtofont --sources ./src/lib/icon/logo --output ./static/font --fontName logo",
    "font:mood": "svgtofont --sources ./src/lib/icon/mood --output ./static/font --fontName mood",
    "font:ui": "svgtofont --sources ./src/lib/icon/ui --output ./static/font --fontName ui",
    "make:tsc": "tsc --target es6 src/lib/scss/bin/*.ts",
    "make:sass": "node src/lib/scss/bin/index.js ./src/lib/scss/*.sass | sort | uniq > src/__spec__/__snapshots__/scss-contrast.txt",
    "make:brotli": "find public -type f -not -name \"*.br\" | grep -v \"dist/font\" | xargs -L1  brotli -Z -f -k"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^1.0.0-next.35",
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "next",
    "@types/node": "^16.4.1",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.3.1",
    "sass": "^1.35.2",
    "svelte": "^3.40.1",
    "svelte-preprocess": "^4.7.4",
    "typescript": "^4.3.5"
  },
  "type": "module",
  "dependencies": {
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.22.2",
    "@editorjs/header": "^2.6.1",
    "@editorjs/link": "^2.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "date-fns": "^2.23.0",
    "dexie": "^3.0.3",
    "editorjs-undo": "^0.1.7",
    "js-yaml": "^4.1.0",
    "moment": "^2.29.1",
    "mongodb": "^4.0.1",
    "pointer-tracker": "^2.4.0",
    "tslib": "^2.3.0",
    "ua-parser-js": "^0.7.28"
  }
}
