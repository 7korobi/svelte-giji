{
  "name": "~TODO~",
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "deploy": "run-s format build",
    "dev": "svelte-kit dev -H",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview -H",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "validate": "run-p format lint",
    "css": "run-s make:sass",
    "json": "run-p json:*",
    "json:game": "rsync -avz ../../giji/giji/app/json/ src/lib/game/json/",
    "json:site": "rsync -avz ../../giji/giji/config/json/ src/lib/site/json/",
    "font": "run-p font:*",
    "font:logo": "svgtofont --sources ./src/lib/icon/logo --output ./static/font --fontName logo",
    "font:mood": "svgtofont --sources ./src/lib/icon/mood --output ./static/font --fontName mood",
    "font:ui": "svgtofont --sources ./src/lib/icon/ui --output ./static/font --fontName ui",
    "make:tsc": "tsc --target es6 src/lib/scss/bin/*.ts",
    "make:sass": "node src/lib/scss/bin/index.js ./src/lib/scss/*.sass | sort | uniq > src/__spec__/__snapshots__/scss-contrast.txt",
    "make:brotli": "find public -type f -not -name \"*.br\" | grep -v \"dist/font\" | xargs -L1  brotli -Z -f -k"
  },
  "devDependencies": {
    "@architect/parser": "^4.0.1",
    "@nikso/adapter-serverless": "^0.1.0",
    "@sveltejs/adapter-begin": "^1.0.0-next.14",
    "@sveltejs/adapter-cloudflare-workers": "^1.0.0-next.22",
    "@sveltejs/adapter-netlify": "^1.0.0-next.31",
    "@sveltejs/adapter-node": "^1.0.0-next.49",
    "@sveltejs/adapter-static": "^1.0.0-next.19",
    "@sveltejs/adapter-vercel": "^1.0.0-next.29",
    "@sveltejs/kit": "next",
    "@types/http-server": "^0.12.1",
    "@types/js-yaml": "^4.0.3",
    "@types/lodash": "^4.14.173",
    "@types/node": "^16.9.3",
    "@types/prettier": "~2.2.3",
    "@types/sass": "^1.16.1",
    "@types/ua-parser-js": "^0.7.36",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.4.0",
    "sass": "^1.41.1",
    "svelte": "^3.42.6",
    "svelte-preprocess": "^4.9.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.22.2",
    "@editorjs/header": "^2.6.1",
    "@editorjs/link": "^2.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "date-fns": "^2.24.0",
    "dexie": "^3.0.3",
    "editorjs-undo": "^0.1.7",
    "firebase": "^9.0.2",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^4.1.2",
    "pointer-tracker": "^2.5.3",
    "tslib": "^2.3.1",
    "ua-parser-js": "^0.7.28"
  }
}
