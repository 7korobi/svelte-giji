{
  "name": "svelte-kit",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "deploy": "run-s format build",
    "test": "run-s jest",
    "jest": "jest",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "start": "svelte-kit start",
    "lint": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "css": "run-s make:sass make:brotli",
    "validate": "run-p format test check",
    "make:model": "ts-node src/make/model.ts",
    "make:schema": "ts-node src/cli/schema.ts",
    "make:sass": "sass --style compressed src/lib/scss:static/css 2>&1 | ruby -pe '$_.sub!(/.+ DEBUG: /,\"\")' | sort | uniq > src/__spec__/__snapshots__/scss-contrast.txt",
    "make:brotli": "find public -type f -not -name \"*.br\" | grep -v \"dist/font\" | xargs -L1  brotli -Z -f -k"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "next",
    "@sveltejs/kit": "next",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/svelte": "^3.0.3",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@types/node-fetch": "^2.5.8",
    "prettier": "~2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "sass": "^1.32.8",
    "svelte": "^3.35.0",
    "svelte-preprocess": "^4.7.0",
    "typescript": "^4.2.3",
    "vite": "^2.1.4"
  },
  "type": "module",
  "engines": {
    "node": ">= 12.17.0"
  },
  "dependencies": {
    "@editorjs/checklist": "^1.3.0",
    "@editorjs/delimiter": "^1.2.0",
    "@editorjs/editorjs": "^2.19.3",
    "@editorjs/header": "^2.6.1",
    "@editorjs/link": "^2.3.1",
    "@editorjs/list": "^1.6.2",
    "@editorjs/marker": "^1.2.2",
    "@editorjs/paragraph": "^2.8.0",
    "@editorjs/quote": "^2.4.0",
    "@sveltejs/adapter-static": "^1.0.0-next.0",
    "dexie": "^3.0.3",
    "editorjs-undo": "^0.1.4",
    "fake-indexeddb": "^3.1.2",
    "fancy-date": "^0.4.1",
    "fastify": "^3.14.1",
    "firebase": "^8.3.1",
    "firebase-admin": "^9.6.0",
    "firebase-functions": "^3.13.2",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "svelte-jester": "^1.3.2",
    "svelte-petit-libs": "^0.0.4",
    "ts-jest": "^26.5.4",
    "tslib": "^2.1.0",
    "ua-parser-js": "^0.7.26"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
        "svelte-jester",
        {
          "preprocess": true
        }
      ],
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "js",
      "ts",
      "svelte"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom/extend-expect"
    ]
  }
}
